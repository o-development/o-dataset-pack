    "example:loadData": "ts-node ./example/loadDataExample.ts",
    "docs": "typedoc",
    "prepublishOnly": "npm run test && npm run build && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/o-development/o-dataset-pack.git"
